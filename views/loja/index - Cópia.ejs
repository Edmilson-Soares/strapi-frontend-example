	<section id="slider"><!--slider-->
		<div class="container">
			<div class="row">
				<div class="col-sm-12">
					<div id="slider-carousel" class="carousel slide" data-ride="carousel">
						<ol class="carousel-indicators">
							<% loja.distaques.forEach(function(destaque,index){ %>
								<% if (index==0) { %>
									<li data-target="#slider-carousel" data-slide-to="0" class="active"></li>
								  <% } %>
								  <% if (index!=0) { %>
									<li data-target="#slider-carousel" data-slide-to="1"></li>
								  <% } %>
							<% }); %>	
						</ol>
						
						<div class="carousel-inner">
							<% loja.distaques.forEach(function(destaque,index){ %>
								<% if (index==0) { %>
									<div class="item active">
								  <% } %>
								  <% if (index!=0) { %>
									<div class="item">
								  <% } %>
							
								<div class="col-sm-6">
									<h1><span>+</span>Proxímo</h1>
									<h2><%= destaque.nome %></h2>
									<p><%= destaque.desc %></p>
									<a href="<%= destaque.link %>">Obter agora</a>
								</div>
								<div class="col-sm-6">
									<img src="<%= destaque.img1 %>" class="girl img-responsive" alt="" />
									<img src="<%= destaque.img2 %>"  class="pricing" alt="" />
								</div>
							</div>
							<% }); %>
							
						</div>
						
						<a href="#slider-carousel" class="left control-carousel hidden-xs" data-slide="prev">
							<i class="fa fa-angle-left"></i>
						</a>
						<a href="#slider-carousel" class="right control-carousel hidden-xs" data-slide="next">
							<i class="fa fa-angle-right"></i>
						</a>
					</div>
					
				</div>
			</div>
		</div>
	</section><!--/slider-->
	
	<section id="app">
		<div class="container">
			<div class="row">
				<div class="col-sm-3">
					<div class="left-sidebar">
						<h2>Categoria</h2>
						<div class="panel-group category-products" id="accordian">

					      <% loja.categorias.forEach(function(categoria,index){ %>
							<div class="panel panel-default">
								<div class="panel-heading">
									<h4 class="panel-title">
										<a data-toggle="collapse" data-parent="#accordian" href="#<%= index %>">
											<span class="badge pull-right"><i class="fa fa-plus"></i></span>
											<%= categoria.nome %>
										</a>
									</h4>
								</div>
								<div id="<%= index %>" class="panel-collapse collapse">
									<div class="panel-body">
										<ul>
											<% categoria.colecoes.forEach(function(colecao){ %>
												<li><a href="#"><%= colecao.nome %></a></li>
											<% }); %>	
										</ul>
									</div>
								</div>
							</div>
							<% }); %>
	

						</div>
					
					</div>
				</div>
				
				<div class="col-sm-9 padding-right">
					<div class="features_items"><!--features_items-->
						<h2 class="title text-center">Caracteristica</h2>
						<h4 class="title text-center">Categória: <%= loja.categorias[0].nome %> Subcategória: <%= loja.categorias[0].colecoes[0].nome %></h4>
						<div class="row" style="margin-top: 20px;">
							<% loja.categorias[0].colecoes[0].produto.forEach(function(produt,index){ %>
							<div class="col-sm-4">
							<div class="product-image-wrapper">
								<div class="single-products">
										<div class="productinfo text-center">
											<img src="<%= produt.img %>" alt="" />
											<h2>€<%= produt.preco %></h2>
											<p><%= produt.nome %></p>
											<a href="product-details.html" class="btn btn-default add-to-cart">ver mais detalhes</a>
											<a href="#" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Adicionar no carrinho</a>
										</div>
										<div class="product-overlay">
											<div class="overlay-content">
												<h2>€<%= produt.preco %></h2>
												<p><%= produt.nome %></p>
												<a href="#" class="btn btn-default add-to-cart">ver mais detalhes</a>
												<a href="#" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Adicionar no carrinho</a>
											</div>
										</div>
								</div>
								<div class="choose">
									<ul class="nav nav-pills nav-justified">
										<li><a href="#"><i class="fa fa-plus-square"></i>Adicionar no favorito</a></li>
										<li><a href="#"><i class="fa fa-plus-square"></i>Adicionar para compra</a></li>
									</ul>
								</div>
							</div>
						</div>
							<% }); %>
						</div>

						
					</div><!--features_items-->
					

					
					<div class="recommended_items"><!--recommended_items-->
						<h2 class="title text-center">Coleções recomedadas</h2>
						
						<div id="recommended-item-carousel" class="carousel slide" data-ride="carousel">
							<div class="carousel-inner">
								<div class="item active">
								<% loja.encomendados.forEach(function(encomendado,index){ %>
									<div class="col-sm-4">
										<div class="product-image-wrapper">
											<div class="single-products">
												<div class="productinfo text-center">
													<img src="images/home/recommend1.jpg" alt="" />
													<h2>€56</h2>
													<p>o nosso produro</p>
													<a href="product-details.html" class="btn btn-default add-to-cart">ver mais detalhes</a>
													<a href="#" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Adicionar no carrinho</a>
												</div>
												
											</div>
										</div>
									</div>
								<% }); %>	
								</div>
		
							</div>
							 <a class="left recommended-item-control" href="#recommended-item-carousel" data-slide="prev">
								<i class="fa fa-angle-left"></i>
							  </a>
							  <a class="right recommended-item-control" href="#recommended-item-carousel" data-slide="next">
								<i class="fa fa-angle-right"></i>
							  </a>			
						</div>
					</div><!--/recommended_items-->
					
				</div>
			</div>
		</div>
